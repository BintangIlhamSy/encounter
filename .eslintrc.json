{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:node/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": ["error",{
            "endOfLine":"crlf"
        }],
        "no-unused-vars":"warn",
        "linebreak-style":["error", "windows"],
        "no-console":"off"
    }
}
